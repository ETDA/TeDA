using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.IO;
using TedaLibrary.Implement;

namespace TedaLibrary.Test
{
    class TestVerifyPdf
    {
        static void Main(string[] args)
        {
            PdfCreator pdfCreator = new PdfCreator();
            string pdfPath = @"";
